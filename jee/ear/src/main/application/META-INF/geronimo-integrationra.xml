<?xml version="1.0" encoding="UTF-8"?>
<connector xmlns="http://geronimo.apache.org/xml/ns/j2ee/connector-1.2">

	<dep:environment xmlns:dep="http://geronimo.apache.org/xml/ns/deployment-1.2">
		<dep:moduleId>
			<dep:groupId>com.sun.xml.bind</dep:groupId>
			<dep:artifactId>jaxb-impl</dep:artifactId>
			<dep:version>2.0.5</dep:version>
			<dep:type>jar</dep:type>
		</dep:moduleId>
	</dep:environment>

	<resourceadapter>
		<resourceadapter-instance>
			<resourceadapter-name>SpringContextResourceAdapter</resourceadapter-name>
			<workmanager xmlns="http://geronimo.apache.org/xml/ns/naming-1.2">
				<gbean-link>DefaultWorkManager</gbean-link>
			</workmanager>
		</resourceadapter-instance>
	</resourceadapter>

</connector>