<configuration xmlns="urn:hornetq"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:hornetq /schema/hornetq-jms.xsd">

	<!--
		| Queues and topics used by the orderplacement application.
	-->

	<topic name="ExternalEvents">
		<entry name="/topic/orderplacement/ExternalEvents" />
	</topic>

	<queue name="ItemCreatedEvents">
		<entry name="/queue/orderplacement/ItemCreatedEvents" />
	</queue>

	<queue name="ItemImportFailures">
		<entry name="/queue/orderplacement/ItemImportFailures" />
	</queue>

</configuration>